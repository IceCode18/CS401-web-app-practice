<div id="pass_<%= pass.id %>" class="card pass" style="width: 18rem;">
    <div class="card-body">
      <h5 class="card-subtitle mb-2 text-muted">ID: <%= link_to_unless_current pass.code_id, pass %></h5>
      <h6 class="card-title">From: <%= pass.from %></p></h6>
      <h6 class="card-subtitle mb-2 text-muted">Expires: <%= pass.expiry %> </h6>
      <p class="card-text"><strong>Code: </strong><%= pass.code %></p>
      <span class="card-link"><%= link_to "Edit", edit_pass_path(pass) %></span>
      <span class="card-link"><%= link_to "Delete", pass, method: :delete, data: {confirm: "Are you sure you want to delete this?" } %></span>
    </div>
</div>
