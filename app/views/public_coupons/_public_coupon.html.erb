<div class="grid-item">
    <div id="coupon_<%= public_coupon.id %>" class="coupon card pass">
        <div class="coupon-main">
            <h2 class= "center"><%= public_coupon.code %></h2>
            <p class="center small">Code</p>
        </div>
        <div class="coupon-info">
            <span class="small f-left"><strong>Expires: </strong> <%= public_coupon.expiry %></span>
            <span class="small f-right"><strong>Claimed by: </strong> <%= public_coupon.claimer %></span>
        </div>
        <div class="coupon-note">
            <p><%= public_coupon.note %></p>
        </div>
        <p class="center small">Note</p>
    </div>
</div>